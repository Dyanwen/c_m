page {
  font-family: W3;
  height: 100%;
  width: 100%;
  background-color: #f7e6ec;
  --main-color: #cc606c;
}
.container {
  width: 100%;
  height: 100%;
}
.model {
  position: absolute;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  margin: 0 auto;
}
.model_show {
  -webkit-animation: slide-top 0.5s ease-out both;
  animation: slide-top 0.5s ease-out both;
}
.model_hide {
  -webkit-animation: slide-bottom 0.5s ease-out both;
  animation: slide-bottom 0.5s ease-out both;
}
.model .model_img {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.top_black {
  height: 25%;
}
.top_title {
  font-family: W7;
  text-align: center;
  font-size: 80rpx;
  color: var(--main-color);
  font-weight: 900;
  letter-spacing: 10rpx;
}
.top_local {
  margin: 104rpx auto 0;
  font-size: 30rpx;
  color: var(--main-color);
  text-align: center;
  letter-spacing: 5rpx;
  line-height: 50rpx;
}
.top_time {
  margin: 80rpx auto 0;
  height: 25px;
  font-size: 30rpx;
  color: var(--main-color);
  text-align: center;
}
.top_hold {
  font-family: W7;
  margin: auto auto 82rpx;
  font-size: 32rpx;
  color: var(--main-color);
  letter-spacing: -0.8rpx;
  text-align: center;
  font-weight: 900;
}
.no_tips {
  font-family: PingFangSC-Regular;
  font-size: 30px;
  color: var(--main-color);
  text-align: center;
  margin: auto;
}
.main_title {
  margin: 206rpx 88rpx 0;
  font-family: W7;
  font-size: 80rpx;
  color: var(--main-color);
  letter-spacing: 0;
  text-align: justify;
}
.main_entil {
  margin: 8rpx 88rpx 0;
  font-size: 32rpx;
  color: var(--main-color);
  letter-spacing: 0;
  text-align: justify;
}
.invite_title {
  margin: 60rpx 88rpx 0;
  font-family: W7;
  font-size: 32rpx;
  color: var(--main-color);
  text-align: justify;
}
.invite_text {
  margin: 34rpx 88rpx 0;
  font-size: 28rpx;
  color: var(--main-color);
  line-height: 48rpx;
}
.invite_img {
  width: 574rpx;
  border-radius: 20rpx;
  margin: 40rpx auto;
  overflow: hidden;
  height: 423rpx;
}
.invite_img image {
  width: 100%;
  height: 100%;
}
.form {
  margin: 80rpx 88rpx 0;
  padding-bottom: 80rpx;
}
.form_item {
  margin-bottom: 48rpx;
}
.form_item .title {
  font-size: 32rpx;
  font-weight: 900;
  color: var(--main-color);
  margin-bottom: 10rpx;
}
.form_item .radio_item {
  display: flex;
  height: 75rpx;
  line-height: 75rpx;
}
.form_item .radio_item view {
  font-size: 30rpx;
  color: var(--main-color);
}
.form button {
  height: 80rpx;
  border-radius: 0rpx;
  min-width: 100%;
  background-image: linear-gradient(270deg, #ce810d 0%, var(--main-color) 100%);
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #ffffff;
  line-height: 48rpx;
  margin-top: 100rpx;
}
.review {
  margin: 200rpx 88rpx 0;
  display: flex;
  flex-direction: column;
}
.review image {
  width: 80rpx;
  height: 80rpx;
  left: calc(50% - 40rpx);
  margin: auto;
}
.review .title {
  margin-top: 40rpx;
  font-size: 40rpx;
  font-weight: 900;
  color: var(--main-color);
  text-align: center;
}
.review .des {
  margin-top: 10rpx;
  font-size: 24rpx;
  color: var(--main-color);
  text-align: center;
  line-height: 36rpx;
  margin-bottom: 50rpx;
}
.review .list {
  color: var(--main-color);
  font-size: 24rpx;
  line-height: 50rpx;
  font-weight: 900;
  text-align: center;
}

@-webkit-keyframes slide-top {
  0% {
    -webkit-transform: translateY(100%);
    transform: translateY(100%);
  }
  100% {
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
}
@keyframes slide-top {
  0% {
    -webkit-transform: translateY(100%);
    transform: translateY(100%);
  }
  100% {
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
}
@-webkit-keyframes slide-bottom {
  0% {
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
  100% {
    -webkit-transform: translateY(100%);
    transform: translateY(100%);
  }
}
@keyframes slide-bottom {
  0% {
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
  100% {
    -webkit-transform: translateY(100%);
    transform: translateY(100%);
  }
}

.metting_model {
  position: relative;
  margin: 50rpx 88rpx 0;
  min-height: 100rpx;
  padding: 80rpx 0;
}
.metting_model .line {
  background-image: linear-gradient(270deg, #ce810d 0%, var(--main-color) 100%);
  width: 2rpx;
  height: 100%;
  position: absolute;
  left: calc(50% - 1rpx);
  top: 0rpx;
  z-index: -1;
}

.metting_model .item {
  position: relative;
  width: 100%;
  display: flex;
  margin: 40rpx 0;
}
.metting_model .item .item_text {
  width: 50%;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: var(--main-color);
  text-align: center;
  line-height: 44rpx;
  padding: 0 20rpx;
}
.metting_model .item image {
  position: absolute;
  left: calc(50% - 12rpx);
  top: 10rpx;
  width: 24rpx;
  height: 24rpx;
  transform: rotate(-315deg);
  background: var(--main-color);
}
.map {
  margin: 80rpx 88rpx 0;
  width: 574rpx;
  height: 480rpx;
}
.map_line {
  margin: 0 88rpx 62rpx;
  height: 6rpx;
  background-image: linear-gradient(270deg, #ce810d 0%, var(--main-color) 100%);
}
.address_item {
  display: flex;
  margin: 0 88rpx 28rpx;
}
.address_item image {
  width: 44rpx;
  height: 44rpx;
}
.address_item view {
  margin-left: 60rpx;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: var(--main-color);
  line-height: 44rpx;
}
.form {
  margin: 80rpx 88rpx 0;
}
.form_item {
  margin-bottom: 48rpx;
}
.form_item view {
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: var(--main-color);
}
.form_item input {
  border: 1px solid #ff99ac59;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: var(--main-color);
  padding: 18rpx 20rpx;
  margin-top: 16rpx;
}

.review {
  margin: 200rpx 88rpx 0;
  display: flex;
  flex-direction: column;
}
